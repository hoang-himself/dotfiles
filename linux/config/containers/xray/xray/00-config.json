{
  "log": {
    "loglevel": "warning"
  },
  "stats": {},
  "policy": {
    "system": {
      "statsInboundDownlink": true,
      "statsInboundUplink": true
    }
  },
  "api": {
    "tag": "api",
    "services": [
      "HandlerService",
      "LoggerService",
      "StatsService",
      "ReflectionService"
    ]
  },
  "dns": {
    "tag": "dns",
    "hosts": {
      "cloudflare-dns.com": "adguard.hoang-himself.dev",
      "dns.google": "adguard.hoang-himself.dev",
      "dns.cloudflare.com": "adguard.hoang-himself.dev",
      "doh.dns.sb": "adguard.hoang-himself.dev",
      "doh.opendns.com": "adguard.hoang-himself.dev",
      "common.dot.dns.yandex.net": "adguard.hoang-himself.dev"
    },
    "servers": [
      "10.88.53.53"
    ],
    "disableCache": true
  },
  "routing": {
    "rules": [
      {
        "type": "field",
        "port": 53,
        "network": "tcp,udp",
        "outboundTag": "dns"
      },
      {
        "type": "field",
        "inboundTag": [
          "api"
        ],
        "outboundTag": "api"
      },
      {
        "type": "field",
        "ip": [
          "10.88.0.0/16"
        ],
        "outboundTag": "freedom"
      },
      {
        "type": "field",
        "ip": [
          "geoip:private"
        ],
        "outboundTag": "blackhole"
      },
      {
        "type": "field",
        "protocol": [
          "bittorrent"
        ],
        "outboundTag": "blackhole"
      }
    ]
  },
  "inbounds": [
    {
      "tag": "api",
      "listen": "127.0.0.1",
      "port": 51820,
      "protocol": "dokodemo-door",
      "settings": {
        "address": "127.0.0.1"
      }
    }
  ],
  "outbounds": [
    {
      "tag": "freedom",
      "protocol": "freedom"
    },
    {
      "tag": "blackhole",
      "protocol": "blackhole"
    },
    {
      "tag": "dns",
      "protocol": "dns",
      "settings": {
        "address": "10.88.53.53"
      }
    }
  ]
}
